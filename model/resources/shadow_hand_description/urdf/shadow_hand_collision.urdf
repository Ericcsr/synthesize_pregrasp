                <box size="0.08 0.03 0.08"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0.0"/>
            <mass value="0.4154"/>
            <inertia ixx="1e-4" ixy="0.0" ixz="0.0" iyy="1e-4" iyz="0.0" izz="1e-4"/>
        </inertial>
    </link>

